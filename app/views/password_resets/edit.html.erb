<h1>新しいパスワードの設定</h1>
<%= form_with model: @user, url: password_reset_path(params[:id]),method: :patch do |f| %>
  <%= f.label :password, "新しいパスワード" %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation, "確認用パスワード" %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "パスワード変更" %>
<% end %>
