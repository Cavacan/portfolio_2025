<h1>アカウント作成</h1>

<p>パスワード</p>
<%= form_with model: @user, url: signin02_create_path, local: true do |f| %>
  <div class="form-item">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form-item">
    <%= f.label :password %>
    <%= f.text_field :password %>
  </div>
  <div class="form-item">
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
  </div>
  <div class="form-item">
    <%= f.submit 'アカウント作成' %>
  </div>
  <% end %>
